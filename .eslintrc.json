{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parser": "@babel/eslint-parser",
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }]
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
